<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>--project-company</key>
	<string>CompanyName</string>
	<key>--project-version</key>
	<string>1.2</string>
	<key>--company-id</key>
	<string>example.com</string>
	<key>--docset-atom-filename</key>
	<string>%PROJECT.atom</string>
	<key>--docset-feed-url</key>
	<string>https://docs.%COMPANYID/%PROJECT/%DOCSETATOMFILENAME</string>
	<key>--docset-package-url</key>
	<string>https://docs.%COMPANYID/%PROJECT/%DOCSETPACKAGEFILENAME</string>
	<key>--docset-fallback-url</key>
	<string>https://docs.%COMPANYID/%PROJECT/</string>
	<key>--create-docset</key>
	<true/>
	<key>--publish-docset</key>
	<true/>
	<key>--install-docset</key>
	<true/>
	<key>--logformat</key>
	<string>xcode</string>
	<key>--keep-intermediate-files</key>
	<true/>
	<key>--no-repeat-first-par</key>
	<true/>
	<key>--ignore</key>
	<array>
		<string>*.m</string>
		<string>Documentation</string>
		<string>Pods/TestStudio</string>
		<string>Pods/Headers/TestStudio</string>
		<string>Pods/BuildHeaders/TestStudio</string>
	</array>
	<key>--index-desc</key>
	<string>README.md</string>
	<key>--include</key>
	<array>
		<string>./README.md</string>
	</array>
	<key>--warn-invalid-crossref</key>
	<false/>
</dict>
</plist>
